#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sonvu/BB/ticket-common/node_modules/.pnpm/node-nats-streaming@0.3.2/node_modules/node-nats-streaming/examples/node_modules:/Users/sonvu/BB/ticket-common/node_modules/.pnpm/node-nats-streaming@0.3.2/node_modules/node-nats-streaming/node_modules:/Users/sonvu/BB/ticket-common/node_modules/.pnpm/node-nats-streaming@0.3.2/node_modules:/Users/sonvu/BB/ticket-common/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sonvu/BB/ticket-common/node_modules/.pnpm/node-nats-streaming@0.3.2/node_modules/node-nats-streaming/examples/node_modules:/Users/sonvu/BB/ticket-common/node_modules/.pnpm/node-nats-streaming@0.3.2/node_modules/node-nats-streaming/node_modules:/Users/sonvu/BB/ticket-common/node_modules/.pnpm/node-nats-streaming@0.3.2/node_modules:/Users/sonvu/BB/ticket-common/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../node-nats-streaming/examples/stan-sub.js" "$@"
else
  exec node  "$basedir/../node-nats-streaming/examples/stan-sub.js" "$@"
fi
